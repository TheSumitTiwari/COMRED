    <div class=" ui basic modal <%=id %>">
        <div class="ui header">
            Report Log
        </div>
        <div class="content">
            <div class="ui right aligned grid">
                <div class="left floated left aligned five wide column">
                   <h3> Name of the Person : <%= name %></h3>
                </div>
                <div class="right floated right aligned seven wide column">
                    <h3>Date and Time : <%= date %></h3>
                </div>
            </div>
            <h3>Input Symptoms :
            </h3>
            <div class="ui grid">
                <% for(var i=0; i<inputArr.length; i++) { %>
                    <div class="four wide column">
                        <div class="ui segment" style="color: black; text-align: center;">
                            <%= symptoms[i] %>
                        </div>
                    </div>
                    <% } %>
            </div>
            <div class="ui negative message">
                <div class="header">
                    <div class="ui right aligned grid">
                        <div class="left floated left aligned six wide column">
                            Result :
                        </div>
                        <div class="right floated right aligned six wide column">
                            <%= output %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="actions">
            <div class="ui red basic cancel inverted button">
                <i class="remove icon"></i>
                Close
            </div>

        </div>
    </div>